    "node-sass-chokidar": "^0.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-storybook": "^1.0.0"
  },
  "devDependencies": {
    "react-scripts": "1.0.14",
    "@storybook/react": "^3.2.12",
    "@storybook/addon-actions": "^3.2.12",
